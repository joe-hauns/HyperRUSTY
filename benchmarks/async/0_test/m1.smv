MODULE main
	VAR
		secret: boolean;
		line: 1..3;
		x: 0..3;
		y: 0..3;
		
	ASSIGN

	init(secret) := TRUE;
	next(secret) ;= {TRUE,FALSE};
	-- init(line) := 1;
	init(line) := 1;
	next(line) := 
		case
			(line < 3) : {1,2}; 
			(line=2) : {3}; 
			TRUE: line;
		esac;

	init(x) := 3;
	next(x) := 3;

	init(y) := 3;
	next(y) := (y);


	DEFINE
		obs_print := (line=2) | (line=3);
		halt := (line=3);